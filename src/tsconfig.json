{
    "compileOnSave": false,
    "compilerOptions": {
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "outDir": "../dist/",
        "rootDir": ".",
        "inlineSources": true,
        "sourceMap": true
    },
    "fileGlobs": [
        "./src/**/*.ts"
    ]
}
